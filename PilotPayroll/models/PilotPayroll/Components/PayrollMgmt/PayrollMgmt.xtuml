INSERT INTO SPR_PS
	VALUES ("2d2951f1-f7bf-4ff2-91bf-5d9b221a1528",
	'SubmitItemApproval',
	'Approve a payment element for an employee on a department payroll.
Preliminary Use Cases only approve hold status.
',
	'// Find the payroll under review; approve the submitted item.
select any dept from instances of Department
 where ( selected.Name == param.Department );
if ( not_empty dept )
  select one payroll related by dept->Payroll[R34.''has under review''];
  if ( not_empty payroll )
  	payroll.ApproveItem( ID:param.EmployeeID, Label:param.PaymentLabel );
  else
    LOG::LogFailure( message:"Payroll not found when updating item approval" );
  end if;
end if;',
	1,
	0,
	0);
