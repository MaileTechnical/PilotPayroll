INSERT INTO SPR_PS
	VALUES ("0bda7c67-dbef-4adf-bb45-59d4e6a9ca9f",
	'RetrievePayrollForReview',
	'Request from client to get department payroll data for review.
Some reviewers request only those items with on-hold status.
',
	'select any dept from instances of Department
 where ( selected.Name == param.Department );
if ( not_empty dept )
  select one payroll related by dept->Payroll[R34.''has under review''];
  if ( not_empty payroll )
  	payroll.SendPayrollForReview( HoldsOnly:param.HoldsOnly );
  end if;
end if;
',
	1,
	0,
	0);
